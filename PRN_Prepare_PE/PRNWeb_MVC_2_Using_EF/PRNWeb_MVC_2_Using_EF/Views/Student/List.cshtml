@{
}
@using Library.Models
@model List<Student>;

<!doctype html>
<html>
<head>
    <title>Student List</title>
</head>
<body>
    <h1>List of students</h1>
    @foreach (Student s in Model)
    {
        <h4 style="color: blue">Id: @(s.StudentId)  Name: @(s.StudentName)</h4>
        <a href="/Student/Detail/@s.StudentId" >Detail</a>
    }
</body>
</html>



