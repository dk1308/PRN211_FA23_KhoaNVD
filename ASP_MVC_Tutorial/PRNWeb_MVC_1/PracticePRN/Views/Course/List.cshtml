@{
}
@model List<String>
<!doctype html>
<html>
    <head>
        <title>@ViewData["title"]</title>
    </head>
    <body>
        <p>@ViewBag.Content</p>
    @{
        <h2>List of courses: </h2>
        foreach(string course in Model)
        {
            <h3>@course</h3>
        }
        }
    </body>
</html>